package extra;

public class RSF {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int input1 = 5271;
		
		int temp = input1;
		int i = 0;
//		while(temp>99)
		{
			
		}
		
		int[]arr = new int[10];
		while(temp!=0)
		{
			arr[i] = temp%10;
			temp = temp/10;
			i++;
		}
		for(i=0;i<arr.length;i++)
		{
			System.out.print(arr[i]+"\t");
		}
		
	}

}
